{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(css|js))",
      "dest": "/static/$1"
    }
  ]
}