{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "main.py" },
    { "src": "/admin.html", "dest": "static/admin.html" },
    { "src": "/(.*)", "dest": "static/$1" }
  ]
}